<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netcoreapp3.1</TargetFramework>
    <Authors>Uchidero Krauch</Authors>
    <Description>Telegram bot implementation</Description>
    <Copyright>Uchidero Krauch Â©</Copyright>
    <RepositoryUrl>https://github.com/asceso/TelegramSimpleService</RepositoryUrl>
    <RepositoryType>Github</RepositoryType>
    <PackageProjectUrl>https://github.com/asceso/TelegramSimpleService</PackageProjectUrl>
    <PackageIcon>package.png</PackageIcon>
    <PackageIconUrl />
    <FileVersion>1.0.0.1</FileVersion>
